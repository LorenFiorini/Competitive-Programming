

def solve():
	n, k = input().split()
	int(n)
	int(k)
	s = str(input())
	score = 0
	for i in range(len(s) // 2):
		if s[i] != s[-i]:
			score += 1
	
	return abs(k - score)


def main():
	t = int(input())
	for _ in range(1, t + 1):
		print("Case #{}: {}".format(_, solve()))
	




if __name__ == '__main__':
	main()


