#include <bits/stdc++.h>
using namespace std;

int main() 
{
	vector<long long> v;
	for (int i = 0; i < 20; i++) {
		string s;
		cin >> s;
		for (int j = 0; j < s.size(); j++) {
			long long num = s[j] - '0';
			v.push_back(num);
		}
	}
	
	long long max = 0;	
	for (int i = 0; i < v.size() - 13; i++) {
		long long p = accumulate(v.begin() + i, v.begin() + i + 13, 1, multiplies<int>());
		if (p > max) {
			max = p;
		}
	}
	
	cout << max ;
	
}



